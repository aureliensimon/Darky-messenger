{
  "manifest_version": 1,
  "name": "Darky Messenger",
  "version": "0.1",
  "description": "Sombre theme pour sombre application",
  "homepage_url": "https://github.com/aureliensimon/darkymessenger",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.messenger.com/*"],
      "css": ["style.css"]
    }
  ]
}
